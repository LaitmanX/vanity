{
  "style": "default",
  "rsc": true,
  "tsx": true,
  "tailwind": {
    "config": "tailwind.config.js",
    "css": "lib/globals.css",
    "baseColor": "slate",
    "cssVariables": true
  },
  "aliases": {
    "components": "@/lib/components",
    "utils": "@/lib/utils",
    "hooks": "@/lib/hooks"
  },
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "components/*": ["lib/components/*"],
      "utils/*": ["lib/utils/*"],
      "lib/*": ["lib/*"],
      "hooks/*": ["lib/hooks/*"],
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/**/**/*.ts",
    "**/**/**/*.tsx",
    ".next/types/**/*.ts",
    "postcss.config.js",
    "next.config.js",
    "tailwind.config.js",
    "lib/context.tsx"
  ],
  "exclude": ["node_modules"]
}
